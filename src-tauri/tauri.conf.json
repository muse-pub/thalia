{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5173",
    "distDir": "../build"
  },
  "package": {
    "productName": "muse.pub",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "2023 Muse Corp",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "../static/icons/32x32.png",
        "../static/icons/128x128.png",
        "../static/icons/128x128@2x.png",
        "../static/icons/icon.ico"
      ],
      "identifier": "com.muse.pub",
      "longDescription": "muse.pub - a new way to create",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "systemTray": {
      "iconPath": "../static/icons/icon.ico",
      "iconAsTemplate": true
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 720,
        "resizable": true,
        "title": ":: MUSE - Development Version ::",
        "width": 1280,
        "fileDropEnabled": true,
        "titleBarStyle": "Transparent"
        
      }
    ]
  }
}
